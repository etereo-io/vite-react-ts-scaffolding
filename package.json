{
  "name": "vite-react-ts-scaffolding",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:deps": "npx depcruise src",
    "types:check": "tsc --noEmit",
    "git:pre-commit": "npm run types:check && npm run lint:deps && lint-staged",
    "test:base": "vitest --passWithNoTests",
    "test": "npm run test:coverage",
    "test:ui": "npm run test:base -- --ui",
    "test:run": "npm run test:base -- --run",
    "test:coverage": "npm run test:base -- --run --coverage",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "(src)/**/*.(ts|tsx)": ["biome check --write"]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@mui/x-charts": "^7.6.1",
    "@tanstack/react-query": "^5.40.1",
    "axios": "^1.7.2",
    "deepmerge-ts": "^7.0.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "intersection-observer": "^0.12.2",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@juggle/resize-observer": "^3.4.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "dependency-cruiser": "^16.3.2",
    "husky": "^9.0.11",
    "install": "^0.13.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "matchmedia-polyfill": "^0.3.2",
    "msw": "^2.3.1",
    "react-remove-attr": "^0.0.6",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.6.0",
    "vitest-sonar-reporter": "^2.0.0"
  }
}
